                <mesh filename="package://robot_description/meshes/gripper_lite.stl" scale="1.0 1.0 1.0"/>                  
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <joint name="base_link_to_link_base" type="fixed">
        <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="link_base"/> 
    </joint>  

    <joint name="link_base_to_link1" type="revolute">
        <origin xyz="0 0 0.24" rpy="0.0 0.0 0.0"/>
        <parent link="link_base"/>
        <child link="link1"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="0" velocity="0"/>
    </joint> 
    
    <joint name="link1_to_link2" type="revolute">
        <origin xyz="0 0.0 0.0" rpy="-1.57 -1.57 0.0"/>
        <parent link="link1"/>
        <child link="link2"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-1.31" upper="1.31" effort="0" velocity="0"/>
    </joint> 
      <joint name="link2_to_link3" type="revolute">
        <origin xyz="0.2 0 0" rpy="3.14 0 1.57"/>
        <parent link="link2"/>
        <child link="link3"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-0.03" upper="2.62" effort="0" velocity="0"/>
    </joint>

      <joint name="link3_to_link4" type="revolute">
        <origin xyz="0.09 -0.23 0" rpy="1.57 6.28 0"/>
        <parent link="link3"/>
        <child link="link4"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="0" velocity="0"/>
    </joint>

      <joint name="link4_to_link5" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 3.14"/>
        <parent link="link4"/>
        <child link="link5"/> 
        <axis xyz="0 1 0"/>
        <limit lower="-1.08" upper="1.08" effort="0" velocity="0"/>
    </joint>

    <joint name="link5_to_link6" type="revolute">
        <origin xyz="0 0 0.03" rpy="0 0 3.14"/>
        <parent link="link5"/>
        <child link="link6"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="0" velocity="0"/>
    </joint>

    <joint name="link6_to_gripper_lite" type="revolute">
        <origin xyz="0 0 0.01" rpy="0 0 3.14"/>
        <parent link="link6"/>
        <child link="gripper_lite"/> 
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="0" velocity="0"/>
    </joint>
