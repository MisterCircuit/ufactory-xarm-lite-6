<robot name="qrobott">
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>
    <link name="base_link">
        <visual>
            <origin xyz="0.01 0.01 0.01" rpy="0 0 0"/>
            <geometry>
                <box size="0 0 0"/>
            </geometry> 
        </visual>
    </link> 

    <link name="link_base">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>                
                <mesh filename="package://robot_description/meshes/link_base.stl" scale="1.0 1.0 1.0"/>                  
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <link name="link1">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>                
