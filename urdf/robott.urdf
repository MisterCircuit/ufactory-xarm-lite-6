            <geometry>                
                <mesh filename="package://robot_description/meshes/link4.stl" scale="1.0 1.0 1.0"/>                  
            </geometry>
            <material name="white"/>
        </visual>
    </link>

    <link name="link5">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>                
                <mesh filename="package://robot_description/meshes/link5.stl" scale="1.0 1.0 1.0"/>                  
            </geometry>
